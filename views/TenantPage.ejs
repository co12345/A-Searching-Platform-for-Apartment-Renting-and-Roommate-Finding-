        <div class ="formtable">
            <form class="form-horizontal"  action="/TenantPage" method="POST">
                <fieldset>
                    <div id="tableTitle">
                        <h1>Register</h1>
                    </div>



                    <!-- Text input-->
                    <div class="form-group">
                        <label class="col-md-4 control-label" for="textinput0">Name</label>
                        <div class="col-md-4">
                            <input id="textinput0" name="name" type="text" placeholder="Name" class="form-control input-md">

                        </div>
                    </div>

                    <!-- Text input-->
                    <div class="form-group">
                        <label class="col-md-4 control-label" for="textinput1">Email Address</label>
                        <div class="col-md-4">
                            <input id="textinput1" name="email" type="text" placeholder="Email" class="form-control input-md">

                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-md-4 control-label" for="selectbasic">Gender</label>
                        <div class="col-md-4">
                            <select id="selectbasic" name="gender" class="form-control">
